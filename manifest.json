{
  "manifest_version": 2,
  "name": "Speedy",
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "bookmarks",
    "chrome://favicon/",
    "management"
  ]
}